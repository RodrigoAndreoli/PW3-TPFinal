<!DOCTYPE html>

<html>
<head>
    <!-- General -->
    <link rel="stylesheet" href="~/Content/css/styles.css" />
    <script type="text/javascript" src="~/Content/js/scripts.js"></script>

    <!-- ##### UI KIT #####-->
    <link rel="stylesheet" href="~/Content/css/uikit/uikit.min.css" />
    <link rel="stylesheet" href="~/Content/css/uikit/uikit-rtl.min.css" />
    <script type="text/javascript" src="~/Content/js/uikit/uikit.min.js"></script>
    <script type="text/javascript" src="~/Content/js/uikit/uikit-icons.min.js"></script>
    <!-- #####      ##### -->
    <!-- ##### FONT AWESOME #####-->
    <link rel="stylesheet" href="~/Content/css/fontawesome/all.min.css" />
    <link rel="stylesheet" href="~/Content/css/fontawesome/fontawesome.min.css" />
    <script type="text/javascript" src="~/Content/js/fontawesome/all.min.js"></script>
    <script type="text/javascript" src="~/Content/js/fontawesome/fontawesome.min.js"></script>
    <script type="text/javascript" src="~/Content/js/fontawesome/brands.min.js"></script>
    <script type="text/javascript" src="~/Content/js/fontawesome/regular.min.js"></script>
    <script type="text/javascript" src="~/Content/js/fontawesome/solid.min.js"></script>
    <!-- #####      ##### -->
    @RenderSection("css")

    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width" />
    @RenderSection("meta")

</head>
<body>
    @RenderSection("nav", true)


    <div>
        @RenderBody()
    </div>
    @RenderSection("js")
</body>
</html>